<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
  <log4net>
    <appender name="PlayerLogger" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\Signage\DeviceLogs\Player_Log_%env{USERNAME}_%date{yyyyMMdd}.log" />
      <encoding value="utf-8" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <LockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <header type="log4net.Util.PatternString" value="[Log Starts]%newline" />
        <footer type="log4net.Util.PatternString" value="[Log Ends]%newline%newline" />
        <conversionPattern value="%date [%username] - %message%newline" />
      </layout>
    </appender>
 
    <appender name="ConnectorLogger" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\Signage\DeviceLogs\Connector_Log_%env{USERNAME}_%date{yyyyMMdd}.log" />
      <encoding value="utf-8" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <LockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <header type="log4net.Util.PatternString" value="[Log Starts]%newline" />
        <footer type="log4net.Util.PatternString" value="[Log Ends]%newline%newline" />
        <conversionPattern value="%date [%username] - %message%newline" />
      </layout>
    </appender>
    <logger name="Player">
      <level value="DEBUG" />
      <appender-ref ref="PlayerLogger" />
    </logger>
 
    <logger name="Connector">
      <level value="DEBUG" />
      <appender-ref ref="ConnectorLogger" />
    </logger>
  </log4net>
  <appSettings>
    <!--<add key="WebApiAddress" value="http://digitalsignageazure.azurewebsites.net" />
    <add key="ScopeId" value="0ne0000DC68"/>-->
    <add key="WebApiAddress" value="https://nodeserverappso5pa.azurewebsites.net " />
